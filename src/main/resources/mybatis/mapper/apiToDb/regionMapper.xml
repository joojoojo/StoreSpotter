<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sojoo.StoreSpotter.dao.apiToDb.RegionMapper">
    <select id="selectRegionList" parameterType="com.sojoo.StoreSpotter.dto.apiToDb.Region">
        SELECT region_id, region_name
        FROM region
    </select>

    <select id="selectSidoCode" parameterType="com.sojoo.StoreSpotter.dto.apiToDb.Region">
        SELECT region_id
        FROM region
        WHERE region_name = #{region_name}
    </select>
</mapper>